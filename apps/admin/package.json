{
  "name": "admin",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "local": "env-cmd -f .env.on-local next dev",
    "prebuild": "next telemetry disable",
    "build:local": "env-cmd -f .env.on-local next build",
    "build:dev": "env-cmd -f .env.development next build",
    "build:stg": "env-cmd -f .env.stage next build",
    "build:prod": "env-cmd -f .env.production next build",
    "start:local": "env-cmd -f .env.on-local next start",
    "start:dev": "env-cmd -f .env.development next start",
    "start:stg": "env-cmd -f .env.stage next start",
    "start:prod": "env-cmd -f .env.production next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:ci": "jest --ci",
    "clean": "rm -rf ./node_modules && rm -rf .swc && rm -rf __tests__ && pnpm clean:build",
    "clean:build": "rm -rf ./.next && rm -rf ./.turbo"
  },
  "dependencies": {
    "next": "13.4.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "13.4.13",
    "@swc/plugin-styled-components": "^1.5.119",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "18.18.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "env-cmd": "^10.1.0",
    "eslint-config-next": "13.4.13",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "styled-components": "^6.1.8",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.15.4",
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "pnpm lint"
    ]
  }
}
